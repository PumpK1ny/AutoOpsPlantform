# QQ机器人配置
QQ_BOT_APPID=your_qq_bot_appid_here
QQ_BOT_SECRET=your_qq_bot_secret_here

# 智谱AI配置
# 支持多API密钥配置（不同账号，无并发限制），使用英文逗号分隔
ZHIPU_API_KEY=your_zhipu_api_key_here

ZHIPU_DEFAULT_MODEL=glm-4.7-flash

# QQ对话模型配置（根据是否有图片切换）
QQCHAT_TEXT_MODEL=glm-4.7-flash      # 纯文本对话模型
QQCHAT_VISION_MODEL=glm-4.6v-flash   # 图片理解模型

# 可选配置
ZHIPU_API_URL=https://open.bigmodel.cn/api/paas/v4
# 默认最大token数配置
ZHIPU_DEFAULT_MAX_TOKENS=100000
# QQ聊天机器人最大token数配置
QQ_BOT_MAX_TOKENS=1024
# 默认温度配置
ZHIPU_DEFAULT_TEMPERATURE=0.8
# 默认top_p配置
ZHIPU_DEFAULT_TOP_P=0.8

# AI Core 额外配置
ZHIPU_DEFAULT_TOOL_CONFIG_PATH=AI/default_tool_config.json
# 默认深度深度思考配置
ZHIPU_ENABLE_DEPTH_THINKING=disable
# 是否显示深度思考内容配置
ZHIPU_SHOW_THINKING_CONTENT=true
# 流式超时配置
ZHIPU_STREAM_TIMEOUT=1000

# QQ机器人目标用户/群组配置
QQ_TARGET_C2C_OPENID=your_target_user_openid_here
QQ_TARGET_GROUP_OPENID=
QQ_BOT_HTTP_API_URL=http://localhost:8080

# Web 服务重启密码
SERVICE_RESTART_PASSWORD=your_restart_password_here
